{
  "name": "@human-id/react-native-humanid",
  "version": "2.3.1",
  "description": "HumanID for react-native",
  "license": "SEE LICENSE IN LISENCE",
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "react-native": "src/index",
  "source": "src/index",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "src",
    "lib",
    "ios",
    "cpp",
    "human-id-react-native-humanid.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "assets/",
    "react-native.config.js"
  ],
  "scripts": {
    "typescript": "tsc --noEmit",
    "prepare": "bob build",
    "release": "release-it"
  },
  "keywords": [
    "react-native",
    "humanid",
    "react-native-humanid",
    "android",
    "ios"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluenumberfoundation/humanid-reactnative-sdk.git"
  },
  "author": {
    "name": "bluenumberfoundation",
    "email": "developers@human-id.org",
    "url": "https://nbs.co.id"
  },
  "contributors": [
    "Bobby Echo (https://github.com/bobbyecho)"
  ],
  "bugs": {
    "url": "https://github.com/bluenumberfoundation/humanid-reactnative-sdk/issues"
  },
  "homepage": "https://github.com/bluenumberfoundation/humanid-reactnative-sdk#readme",
  "devDependencies": {
    "@release-it/conventional-changelog": "2.0.0",
    "@types/react": "17.0.0",
    "@types/react-native": "^0.63.42",
    "react-native-builder-bob": "0.17.1",
    "react-native-device-info": "^7.3.1",
    "release-it": "14.4.1",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "@human-id/react-native-humanid": "^2.3.1",
    "google-libphonenumber": "3.2.15",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-modal": "11.6.1",
    "react-native-webview": "^11.23.1"
  },
  "peerDependencies": {
    "react": ">= 16.8.2",
    "react-native": ">= 0.55.0",
    "react-native-device-info": "^7.3.1"
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true,
      "registry": "https://registry.npmjs.org"
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  }
}
